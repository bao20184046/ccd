PROGRAM BAI4;
VAR INPUT: INTEGER;
PROCEDURE WD(DAY: INTEGER);
	BEGIN
	IF DAY = 1 THEN CALL WRITEC('C')
	ELSE IF DAY = 2 THEN CALL WRITEC('H')
	ELSE IF DAY = 3 THEN CALL WRITEC('B')
	ELSE IF DAY = 4 THEN CALL WRITEC('T')
	ELSE IF DAY = 5 THEN CALL WRITEC('N')
	ELSE IF DAY = 6 THEN CALL WRITEC('S')
	ELSE IF DAY = 7 THEN CALL WRITEC('A')
	ELSE CALL WRITEC('#');
	END;
BEGIN
	INPUT:= READI;
	CALL WD(INPUT);
END.
