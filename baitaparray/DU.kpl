PROGRAM PRO;
VAR A:ARRAY(.10.) OF ARRAY(.10.) OF INTEGER;
	I: INTEGER;
	J: INTEGER;
	N: INTEGER;
	CHECK: INTEGER;
BEGIN
	CALL WRITEC('n');
	CALL WRITEC('=');
	N:= READI;
	FOR I:=0 TO (N-1) DO
		FOR J:=0 TO (N-1) DO
		BEGIN
			CALL WRITEC('A');
			CALL WRITEI(I+1);
			CALL WRITEI(J+1);
			CALL WRITEC('=');
			A(.I.)(.J.):=READI;
		END;
	CHECK:= 1;
	FOR I:=1 TO (N-1) DO
		FOR J:= 0 TO (I-1) DO
		BEGIN
			IF A(.I.)(.J.) != 0 THEN
				CHECK:=0;
		END;
	CALL WRITEI(CHECK);
END.